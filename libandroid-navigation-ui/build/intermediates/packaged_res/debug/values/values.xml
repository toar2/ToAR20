    <declare-styleable name="NavigationMapRoute">
    <!-- Primary route colors -->
    <attr format="color" name="routeColor"/>
    <attr format="color" name="routeModerateCongestionColor"/>
    <attr format="color" name="routeSevereCongestionColor"/>
    <attr format="color" name="routeShieldColor"/>

    <!-- Alternative route colors -->
    <attr format="color" name="alternativeRouteColor"/>
    <attr format="color" name="alternativeRouteModerateCongestionColor"/>
    <attr format="color" name="alternativeRouteSevereCongestionColor"/>
    <attr format="color" name="alternativeRouteShieldColor"/>

    <!-- Origin/Destination waypoint drawables -->
    <attr format="reference" name="originWaypointIcon"/>
    <attr format="reference" name="destinationWaypointIcon"/>

    <!-- Route Scales -->
    <attr format="float" name="routeScale"/>
    <attr format="float" name="alternativeRouteScale"/>

    <attr format="color" name="upcomingManeuverArrowColor"/>
    <attr format="color" name="upcomingManeuverArrowBorderColor"/>
  </declare-styleable>
    <declare-styleable name="NavigationView">
    <!-- Colors -->
    <attr format="color" name="navigationViewPrimary"/>
    <attr format="color" name="navigationViewSecondary"/>
    <attr format="color" name="navigationViewAccent"/>
    <attr format="color" name="navigationViewPrimaryText"/>
    <attr format="color" name="navigationViewSecondaryText"/>
    <attr format="color" name="navigationViewDivider"/>

    <!-- Instruction List -->
    <attr format="color" name="navigationViewListBackground"/>

    <!-- Banner -->
    <attr format="color" name="navigationViewBannerBackground"/>
    <attr format="color" name="navigationViewBannerPrimaryText"/>
    <attr format="color" name="navigationViewBannerSecondaryText"/>
    <attr format="color" name="navigationViewBannerManeuverPrimary"/>
    <attr format="color" name="navigationViewBannerManeuverSecondary"/>

    <!-- Progress Bars -->
    <attr format="color" name="navigationViewProgress"/>
    <attr format="color" name="navigationViewProgressBackground"/>

    <!-- Map style -->
    <attr format="string" name="navigationViewMapStyle"/>

    <!-- Route style -->
    <attr format="reference" name="navigationViewRouteStyle"/>

    <!-- Location layer style -->
    <attr format="reference" name="navigationViewLocationLayerStyle"/>

    <!-- Destination marker drawable -->
    <attr format="reference" name="navigationViewDestinationMarker"/>

    <!-- Route overview drawable -->
    <attr format="reference" name="navigationViewRouteOverviewDrawable"/>

    <!-- For setting the styles in XML -->
    <attr format="reference" name="navigationLightTheme"/>
    <attr format="reference" name="navigationDarkTheme"/>
  </declare-styleable>
